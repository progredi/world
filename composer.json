{
  "name": "progredi/world",
  "description": "World plugin for CakePHP 3",
  "type": "cakephp-plugin",
  "keywords": ["cakephp", "world", "countries", "regions", "continents", "currencies"],
  "homepage": "https://github.com/progredi/world",
  "license": "MIT",
  "authors": [
    {
      "name": "David Scott",
      "email": "support@progredi.co.uk",
      "homepage": "https://www.progredi.co.uk",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.6",
    "cakephp/cakephp": "3.4.*"
  },
  "require-dev": {
    "cakephp/cakephp-codesniffer": "^3.0",
    "phpunit/phpunit": "<6.0"
  },
  "autoload": {
    "psr-4": {
      "Progredi\\World\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cake\\Test\\": "../cakephp/cakephp/tests",
      "Progredi\\World\\Test\\": "tests"
    }
  },
  "minimum-stability": "stable"
}
